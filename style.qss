PollutantCard {
  border: 2px solid gray;
  border-radius: 10px;
  margin-top: 10px;
  padding: 5px;
  font-weight: bold;
  font-size: 14px;
}

PollutantCard[complianceLevel="high"] {
  border: 2px solid green;
  color: darkgreen;
}

PollutantCard[complianceLevel="medium"] {
  border: 2px solid orange;
  color: darkorange;
}

PollutantCard[complianceLevel="low"] {
  border: 2px solid red;
  color: darkred;
}
